  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.1.16",
    "@types/moment-timezone": "^0.5.30",
    "@types/node-fetch": "^2.5.7",
    "moment-timezone": "^0.5.32",
    "node-fetch": "^2.6.1",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@bkper/bkper-api-types": "^3.7.1",
    "@types/request": "^2.48.5",
    "typescript": "^4.1.3"
  }
}
